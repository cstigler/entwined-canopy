{
  "name": "entwined-canopy",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "yarn --cwd client build",
    "lint": "yarn --cwd client lint",
    "start": "yarn --cwd server start",
    "dev": "concurrently --kill-others \"yarn --cwd client serve\" \"yarn --cwd server dev\"",
    "deploy": "git subtree split --prefix server -b deploy && git push -f heroku deploy:main"
  },
  "dependencies": {
    "concurrently": "^5.3.0"
  },
  "devDependencies": {
    "md5": "^2.3.0"
  }
}
